{
  "ext": "ts js",
  "execMap": {
    "ts": "ts-node -P ./tsconfig.build.json"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "watch": [
    "src/server/**/*.ts",
    "src/next/**/*.ts",
    "tsconfig.*.json",
    "tsconfig.json",
    "next.config.js",
    ".babelrc",
    ".env"
  ],
  "ignore": [
    "*.spec.ts"
  ]
}

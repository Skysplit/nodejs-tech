{
  "name": "expressjs-typescript-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "cli": "NODE_ENV=production node -r ./dist/bootstrap",
    "cli:dev": "ts-node -P ./tsconfig.build.json -r ./src/bootstrap",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "npm run serve",
    "start:dev": "nodemon ./src/index.ts",
    "start:debug": "nodemon --inspect ./src/index.ts",
    "prebuild": "npm run build:clean",
    "build": "npm run build:server && npm run build:client",
    "build:clean": "rimraf dist",
    "build:server": "tsc -p ./tsconfig.build.json",
    "build:client": "next build src/client",
    "lint": "tslint --format stylish --project ./tsconfig.json --config ./tslint.json",
    "test": "npm run lint && npm run test:ci && npm run test:coverage",
    "test:ci": "npm run test:client:ci && npm run test:server:ci",
    "test:client": "cross-env NODE_ENV=testing jest --config jest.client.js",
    "test:client:ci": "npm run test:client -- --coverage --runInBand --ci",
    "test:client:watch": "npm run test:client -- --watch",
    "test:server": "cross-env NODE_ENV=testing APP_SECRET=testing jest --config jest.server.js --runInBand",
    "test:server:ci": "npm run test:server -- --coverage --ci",
    "test:server:watch": "npm run test:server -- --watch",
    "test:coverage": "istanbul report json lcov clover text"
  },
  "author": "Tomasz Romik",
  "license": "MIT",
  "engines": {
    "node": "~10.5.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mysql": "^2.16.0",
    "next": "^6.1.1",
    "next-routes": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6",
    "source-map-support": "^0.5.8",
    "tsconfig-paths": "^3.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.2",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.3",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.116",
    "@types/next": "^6.1.2",
    "@types/node": "^10.7.0",
    "@types/passport": "^0.4.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.32",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/supertest": "^2.0.5",
    "@zeit/next-source-maps": "0.0.2",
    "@zeit/next-typescript": "^1.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "istanbul": "^0.4.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  }
}
